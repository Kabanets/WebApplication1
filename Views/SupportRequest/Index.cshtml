@model IEnumerable<WebApplication1.Models.SupportRequest>
@{
    ViewBag.Title = "Supprort requests";
}

<h1>Supprort requests</h1>
<table class="table table-striped">
    <thead>
        <tr>
            <th>Subject</th>
            <th>Date</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.ActionLink(item.Subject, "Details", new { supportRequestId = item.SupportRequestId })
                </td>
                <td>
                    @item.Date.ToString("g")
                </td>
                <td>
                    @item.Email
                </td>
            </tr>
        }
    </tbody>
</table>